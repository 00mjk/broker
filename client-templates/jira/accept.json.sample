{
  "public":
  [],
  "private":
  [
    {
      "//": "identify user",
      "method": "GET",
      "path": "/myself",
      "origin": "https://${JIRA_API}",
      "auth": {
        "scheme": "basic",
        "username": "${JIRA_USERNAME}",
        "password": "${JIRA_PASSWORD}"
      }
    },
    {
      "//": "used to get a list of all Jira projects",
      "method": "GET",
      "path": "/project",
      "origin": "https://${JIRA_API}",
      "auth": {
        "scheme": "basic",
        "username": "${JIRA_USERNAME}",
        "password": "${JIRA_PASSWORD}"
      },
      "valid": [
        {
          "queryParam": "expand",
          "values": [
            "issueTypes"
          ]
        }
      ]
    },
    {
      "//": "used to get a Jira project",
      "method": "GET",
      "path": "/project/:project",
      "origin": "https://${JIRA_API}",
      "auth": {
        "scheme": "basic",
        "username": "${JIRA_USERNAME}",
        "password": "${JIRA_PASSWORD}"
      }
    },
    {
      "//": "used to get createmeta for a jira project & issueType",
      "method": "GET",
      "path": "/issue/createmeta",
      "origin": "https://${JIRA_API}",
      "auth": {
        "scheme": "basic",
        "username": "${JIRA_USERNAME}",
        "password": "${JIRA_PASSWORD}"
      }
    },
    {
      "//": "used to get jira issue",
      "method": "GET",
      "path": "/issue/:issue",
      "origin": "https://${JIRA_API}",
      "auth": {
        "scheme": "basic",
        "username": "${JIRA_USERNAME}",
        "password": "${JIRA_PASSWORD}"
      }
    },
    {
      "//": "used to create jira issue",
      "method": "POST",
      "path": "/issue",
      "origin": "https://${JIRA_API}",
      "auth": {
        "scheme": "basic",
        "username": "${JIRA_USERNAME}",
        "password": "${JIRA_PASSWORD}"
      }
    },
    {
      "//": "used to create jira issue link",
      "method": "POST",
      "path": "/issue/:issue/remotelink",
      "origin": "https://${JIRA_API}",
      "auth": {
        "scheme": "basic",
        "username": "${JIRA_USERNAME}",
        "password": "${JIRA_PASSWORD}"
      }
    },
    {
      "//": "used to get assignable users",
      "method": "GET",
      "path": "/user/assignable/search",
      "origin": "https://${JIRA_API}",
      "auth": {
        "scheme": "basic",
        "username": "${JIRA_USERNAME}",
        "password": "${JIRA_PASSWORD}"
      }
    },
    {
      "//": "used to get available components",
      "method": "GET",
      "path": "/project/:project/components",
      "origin": "https://${JIRA_API}",
      "auth": {
        "scheme": "basic",
        "username": "${JIRA_USERNAME}",
        "password": "${JIRA_PASSWORD}"
      }
    }
  ]
}
